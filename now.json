{
  "name": "melfina",
  "alias": "melfina.network",
  "builds": [{"current": "*.js", "use": "@now/node"}],
  "routes": [
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/",
      "dest": "current/index.js"
    },
    {
      "src": "/ghost",
      "dest": "current/index.js"
    }
  ],
  "version": 2
}